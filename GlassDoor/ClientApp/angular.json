{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ClientApp": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ClientApp",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],

            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              //"src/app/modules/employee/components/employee-profile/css/fontawesome/fontawesome-all.css",
              //"src/app/modules/employee/components/employee-profile/css/font-awesome.min.css",
              //"src/app/modules/employee/components/employee-profile/css/responsive.css",
              //"src/app/modules/employee/components/employee-profile/css/transitions.css",
              //"src/app/modules/employee/components/employee-profile/css/color.css",
              //"src/app/modules/employee/components/employee-profile/css/main.css",
              //"src/app/modules/employee/components/employee-profile/css/chosen.css",
              //"src/app/modules/employee/components/employee-profile/css/scrollbar.css",
              //"src/app/modules/employee/components/employee-profile/css/chartist.css",
              //"src/app/modules/employee/components/employee-profile/css/magnific-popup.css",
              //"src/app/modules/employee/components/employee-profile/css/prettyPhoto.css",
              //"src/app/modules/employee/components/employee-profile/css/linearicons.css",
              //"src/app/modules/employee/components/employee-profile/css/owl.carousel.css",
              //"src/app/modules/employee/components/employee-profile/css/normalize.css",
              "src/styles.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/saga-blue/theme.css",
              "node_modules/primeng/resources/primeng.min.css",
              "node_modules/primeflex/primeflex.css",
              "node_modules/quill/dist/quill.core.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/ngx-toastr/toastr.css"
            ],
            "scripts": [
              "node_modules/quill/dist/quill.js",
              //"src/assets/js/scrollbar.min.js",
              //"src/assets/js/prettyPhoto.js",
              //"src/assets/js/owl.carousel.min.js",
              //"src/assets/js/main.js",
              //"src/assets/js/magnific-popup.js",
              //"src/assets/js/jquery.sortable.js",
              //"src/assets/js/jquery.collapse.js",
              //"src/assets/js/jquery.basictable.min.js",
              //"src/assets/js/gmap3.js",
              //"src/assets/js/countdown.min.js",
              //"src/assets/js/circle-progress.js",
              //"src/assets/js/chosen.jquery.js",
              //"src/assets/js/chartist.js",
              //"src/assets/js/appear.js",

              //"src/assets/js/vendor/bootstrap.min.js",
              //"src/assets/js/vendor/jquery-3.3.1.js",
              //"src/assets/js/vendor/jquery-library.js",
              //"src/assets/js/vendor/modernizr-2.8.3-respond-1.4.2.min.js",
              //"src/assets/js/appear.js"

            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ClientApp:build",
            "publicHost": "localhost:44390"
          },
          "configurations": {
            "production": {
              "browserTarget": "ClientApp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ClientApp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/ngx-toastr/toastr.css"

            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ClientApp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ClientApp:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "ClientApp"
}
